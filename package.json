{
  "name": "csms-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run tokens:build && next dev",
    "build": "npm run tokens:build && next build",
    "postinstall": "npm run tokens:build",
    "start": "next start",
    "lint": "next lint",
    "tokens:build": "node scripts/build-tokens.mjs",
    "tokens:watch": "chokidar \"design/figma/tokens/*.json\" -c \"npm run tokens:build\"",
    "prebuild": "npm run tokens:build",
    "openapi:pull": "curl -fSL $BACKEND_OPENAPI_URL -o ./openapi.json",
    "openapi:gen": "openapi -i ./openapi.json -o ./lib/openapi --client fetch --useOptions --exportServices true --exportModels true",
    "openapi": "dotenv -e .env.local -- npm run openapi:pull && npm run openapi:gen"
  },
  "dependencies": {
