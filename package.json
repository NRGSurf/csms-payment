{
  "name": "csms-webapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run tokens:build && next dev",
    "build": "npm run tokens:build && next build",
    "postinstall": "npm run tokens:build",
    "start": "next start",
    "lint": "next lint",
    "tokens:build": "node scripts/build-tokens.mjs",
    "tokens:watch": "chokidar \"design/figma/tokens/*.json\" -c \"npm run tokens:build\"",
    "prebuild": "npm run tokens:build",
    "openapi:pull": "curl -fSL $BACKEND_OPENAPI_URL -o ./openapi.json",
    "openapi:gen": "openapi -i ./openapi.json -o ./lib/openapi --client fetch --useOptions --exportServices true --exportModels true",
    "openapi": "dotenv -e .env.local -- npm run openapi:pull && npm run openapi:gen"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@hookform/resolvers": "^5.2.1",
    "@mui/material": "^7.2.0",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/themes": "^3.2.1",
    "braintree": "^3.32.0",
    "braintree-web-drop-in": "^1.45.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.541.0",
    "next": "15.4.5",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "ws": "^8.18.3",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/braintree": "^3.4.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/ws": "^8.18.1",
    "chokidar-cli": "^3.0.0",
    "dotenv-cli": "^10.0.0",
    "openapi-typescript-codegen": "^0.29.0",
    "tailwindcss": "^4.1.12",
    "typescript": "^5"
  }
}
